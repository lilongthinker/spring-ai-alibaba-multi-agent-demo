# 云边奶茶铺多智能体系统环境变量模板
# 请根据实际情况修改以下环境变量值

# ==================== AI服务配置 ====================
# DashScope API配置
DASHSCOPE_API_KEY=
DASHSCOPE_MODEL=qwen-plus
DASHSCOPE_INDEX_ID=
DASHSCOPE_ENABLE_RERANKING=true
DASHSCOPE_RERANK_TOP_N=2
DASHSCOPE_RERANK_MIN_SCORE=0

# OpenAI API配置
AI_OPENAI_API_KEY=
AI_OPENAI_BASE_URL=

# Mem0 API配置
MEM0_ADDRESS=https://api.mem0.ai
MEM0_API_KEY=

# ==================== Nacos配置 ====================
# Nacos服务配置
NACOS_SERVER_ADDR=127.0.0.1:8848
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos
NACOS_NAMESPACE=public

# ==================== 数据库配置 ====================
# MySQL数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=multi-agent-demo
DB_USERNAME=multi_agent_demo
DB_PASSWORD=multi_agent_demo@321

# ==================== Spring AI Alibaba Admin 配置 ====================
# Spring AI Alibaba Admin Consult Agent 提示词 Key
PROMPT_KEY=consult-sub-agent-instruction
# Spring AI Alibaba Admin Trace Collector地址
ADMIN_OTLP_ENDPOINT=http://127.0.0.1:4318/v1/traces

# ==================== 任务调度配置 ====================
# XXL-JOB配置
XXL_JOB_ENABLED=false
XXL_JOB_ADMIN=
XXL_JOB_ACCESS_TOKEN=default_token
XXL_JOB_APPNAME=multi-agent-demo
XXL_JOB_DINGTALK_ACCESS_TOKEN=